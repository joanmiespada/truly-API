{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "SQS:SendMessage",
                "sqs:ChangeMessageVisibility",
                "SQS:ReceiveMessage",
                "sqs:DeleteMessage",
                "SQS:GetQueueAttributes"
            ],
            "Resource": "arn:aws:sqs:::*"
        }
    ]
}